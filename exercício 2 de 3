using System;
using System.Collections.Generic;
using System.Linq;
using System.Reflection;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
    internal class Program
    {
        static void Main(string[] args)
        {
            //vari√°vel a 

            int a = 2;
            int[] vetor = new int[6];

            //calculo de a

            while (a < 6) 
            {
                vetor[a] = a * 10;
                Console.WriteLine(vetor[a]);
                a++;                                         
            }
            Console.ReadLine();

        }
    }
}



